            "required": true,
            "autocomplete": true,
            "focused": true
          },
          {
              "type": 4,
              "name": "relic_level",
              "description": "Relic Level (default 5)",
              "required": false,
              "min_value": 1
          },
          {
              "type": 4,
              "name": "gear_level",
              "description": "Gear level (default 13)",
              "required": false,
              "min_value": 1
          },
          {
            "type": 3,
            "name": "allycode_option",
            "description": "primary or alt allycode",
            "required": false,
            "choices": [
              {
                "name": "Primary",
                "value": "primary"
              },
              {
                "name": "Alt",
                "value": "alt"
              }
            ]
          }
        ]
      },
      {
        "type": 1,
        "name": "journey",
        "description": "Check inventory for mats needed for journey guide readiness (requires google link)",
        "options": [
          {
            "type": 3,
