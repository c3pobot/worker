{
  "type": "private",
  "worker": "discord",
  "cmd": {
    "name": "poll",
    "description": "manage server polls",
    "options": [
      {
        "type": 1,
        "name": "create",
        "description": "Start a poll",
        "options": [
          {
            "type": 3,
            "name": "question",
            "description": "poll question",
            "required": true
          },
          {
            "type": 3,
            "name": "responses",
            "description": "poll responses (max 25 seperated by ;)",
            "required": true
          },
          {
            "type": 7,
            "name": "channel",
            "description": "channel to start the poll in"
          }
        ]
      },
      {
        "type": 1,
        "name": "show",
        "description": "repost the active poll in this channel"
      },
      {
        "type": 1,
        "name": "stats",
        "description": "get poll stats",
        "options": [
          {
            "type": 7,
            "name": "channel",
            "description": "channel poll is running in"
          }
        ]
      },
      {
        "type": 1,
        "name": "end",
        "description": "end a running poll",
        "options": [
          {
            "type": 7,
            "name": "channel",
            "description": "channel poll is running id"
          }
        ]
      }
    ]
  }
}
