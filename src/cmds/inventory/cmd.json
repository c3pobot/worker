        "description": "Export inventory to JSON object (requires google link)",
        "options": [
          {
            "type": 3,
            "name": "allycode_option",
            "description": "primary or alt allycode",
            "required": false,
            "choices": [
              {
                "name": "Primary",
                "value": "primary"
              },
              {
                "name": "Alt",
                "value": "alt"
